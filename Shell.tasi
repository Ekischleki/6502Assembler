Name TasiShell;
Type Generic;
Import "unsafe.shell"; 

Start {



	[Console.Clear]

	makeVar string code;
	set code "print('Hello from Python!' + chr(90))";
	[Console.WriteLine:[Shell.Execute:[String.Replace:"echo Add-Type -TypeDefinition '{CODE}' -Language CSharp; [HelloWorld]::Main() | powershell","{CODE}",($code)]]];

	return;
};
